{
	"connectionString": "App-insights-InstrumentationKey",
	"role": {
		"name": "demo name"
	},
	"instrumentation": {
		"logging": {
			"level": "TRACE"
		}
	},
	"selfDiagnostics": {
		"destination":"file+console",
		"level": "DEBUG",
		"file": {
			"path": "applicationinsights.log",
			"maxSizeMb": 5,
			"maxHistory": 1
		}
	},
	"sampling": {
		"percentage": 0,
		"overrides":  [ 
			{
				"telemetryType": "request",
				"attributes": [
				  {
					"key": "url.path",
					"value": ".*/parcours/.*",
					"matchType": "regexp"
				  }
				],
				"percentage": 100
			}
		]
	}
}